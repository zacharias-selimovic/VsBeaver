{
  "name": "VsBeaver",
  "displayName": "Beaver syntax",
  "description": "Simple Beaver syntax highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "publisher": "zache1337",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacharias-selimovic/VsBeaver/tree/main"
  },
  "icon": "assets/vsBeaver.png",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "beaver",
        "aliases": [
          "Beaver",
          "beaver"
        ],
        "extensions": [
          ".beaver"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "beaver",
        "scopeName": "source.beaver",
        "path": "./syntaxes/beaver.tmLanguage.json"
      }
    ]
  }
}