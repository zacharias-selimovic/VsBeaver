{
  "name": "beaver-syntax",
  "displayName": "Beaver syntax",
  "description": "Beaver syntax highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "beaver",
      "aliases": ["Beaver", "beaver"],
      "extensions": [".beaver"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "beaver",
      "scopeName": "source.beaver",
      "path": "./syntaxes/beaver.tmLanguage.json"
    }]
  }
}
